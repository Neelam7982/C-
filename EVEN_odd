using System;

namespace EVEN_odd
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Enter the number");
            int n = int.Parse(Console.ReadLine());    
            if (n % 2 == 0)
            {
                Console.WriteLine("EVEN NUMBER");
            }
            else
            {
                Console.WriteLine("ODD NUMBER");
            }
            Console.ReadLine();
        }
    }
}
